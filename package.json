  "license": "MIT",
  "homepage": "https://github.com/AbrahamTewa/starter.reactjs",
  "private": true,
  "scripts": {
    "analysis": "plato --dir analysis --recurse --eslint .eslintrc.json --title \"Starter server\" src/**/*.js",
    "build": "grunt build",
    "debugger": "devtool build/index.js",
    "doc": "grunt clean:doc && jsdoc -c jsdoc.config.json --readme README.md",
    "lint": "grunt lint",
    "start": "node build/index.js ",
    "test": "jest --config jest.config.json",
    "test:u": "jest -u --config jest.config.json",
    "test-cov": "jest --config jest.config.json --coverage",
    "watch": "webpack-dev-server --progress --inline --hot"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/AbrahamTewa/starter.reactjs"
  },
