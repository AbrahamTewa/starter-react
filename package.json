{
  "name": "starter.nodejs",
  "version": "0.2.0",
  "description": "Starter kit for ReactJS",
  "author": "Abraham Tewa",
  "license": "MIT",
  "homepage": "https://github.com/AbrahamTewa/starter.reactjs",
  "engines": {
    "node": ">=8.9.0"
  },
  "private": true,
  "scripts": {
    "analysis": "grunt clean:analysis && plato --dir reports/analysis --recurse --title \"Starter React\" src/**/*.js",
    "build": "cross-env NODE_ENV=production grunt pre-build && webpack -p",
    "clean": "grunt clean",
    "doc": "npm run doc:jsdoc && npm run doc:storybook",
    "doc:jsdoc": "grunt clean:jsdoc && jsdoc -c ./jsdoc.config.json --readme README.md",
    "doc:storybook": "grunt clean:storybook && build-storybook -c storybook -o doc/storybook",
    "lint": "grunt lint --force",
    "start": "node server.js",
    "test": "jest --config jest.config.json",
    "test-cov": "grunt clean:coverage && jest --config jest.config.json --coverage",
    "watch": "webpack-dev-server --progress --hot -d",
    "watch:storybook": "start-storybook -p 9001 -c storybook"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/AbrahamTewa/starter.reactjs"
  },
  "dependencies": {
    "history": "^4.7.2",
    "prop-types": "^15.6.0",
    "react": "^16.1.0",
    "react-dom": "^16.2.0",
    "react-redux": "^6.0.0",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^4.0.8",
    "redux": "^4.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/polyfill": "^7.2.5",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "@storybook/addon-actions": "^4.1.4",
    "@storybook/addon-storyshots": "^4.1.4",
    "@storybook/react": "^4.1.4",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-istanbul": "^5.1.0",
    "cross-env": "^5.1.1",
    "css-loader": "^2.1.0",
    "enzyme": "^3.1.1",
    "enzyme-adapter-react-16": "^1.0.4",
    "eslint": "^5.11.1",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-jest": "^22.1.2",
    "eslint-plugin-react": "^7.12.0",
    "express": "^4.16.2",
    "grunt": "^1.0.1",
    "grunt-babel": "^8.0.0",
    "grunt-contrib-clean": "^2.0.0",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-contrib-cssmin": "^3.0.0",
    "grunt-eslint": "^21.0.0",
    "grunt-gulp": "^1.0.2",
    "grunt-jsonlint": "^1.1.0",
    "grunt-sass": "^3.0.2",
    "grunt-stylelint": "^0.10.1",
    "grunt-webpack": "^3.0.2",
    "gulp": "^4.0.0",
    "html-webpack-plugin": "^3.2.0",
    "istanbul": "^0.4.5",
    "jest": "^23.6.0",
    "jsdoc": "^3.5.5",
    "jsdoc-oblivion": "^0.0.9",
    "load-grunt-tasks": "^4.0.0",
    "minami": "^1.1.1",
    "node-sass": "^4.11.0",
    "plato": "^1.7.0",
    "react-test-renderer": "^16.1.0",
    "sass-loader": "^7.1.0",
    "sc5-styleguide": "^2.0.4",
    "source-map-support": "^0.5.0",
    "style-loader": "^0.23.1",
    "stylelint": "^9.9.0",
    "stylelint-webpack-plugin": "^0.10.5",
    "uglify-es": "^3.1.9",
    "webpack": "^4.28.3",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.14"
  },
  "optionalDependencies": {
    "fsevents": "*"
  }
}
