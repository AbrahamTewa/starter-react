{
  "name": "starter.nodejs",
  "version": "0.1.0",
  "description": "Starter kit for ReactJS",
  "author": "Abraham Tewa",
  "license": "MIT",
  "homepage": "https://github.com/AbrahamTewa/starter.nodejs.webapp",
  "private": true,
  "scripts": {
    "analysis": "plato --dir analysis --recurse --eslint .eslintrc.json --title \"Starter server\" src/**/*.js",
    "build": "grunt build",
    "debugger": "devtool build/index.js",
    "doc": "grunt clean:doc && jsdoc -c jsdoc.json --readme README.md",
    "linter": "grunt lint",
    "start": "node build/index.js ",
    "test": "mocha src\\**\\__test__\\**\\*.js --opts mocha.opts -R spec",
    "test-cov": "istanbul cover ./node_modules/mocha/bin/_mocha -- src\\**\\__test__\\**\\*.js --opts mocha.opts -R spec",
    "watcher": "grunt watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/AbrahamTewa/starter.reactjs"
  },
  "dependencies": {
    "es6-error": "^4.0.0",
    "prop-types": "^15.5.10",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-redux": "^5.0.5",
    "redux": "^3.6.0"
  },
  "devDependencies": {
    "babel-core": "^6.10.4",
    "babel-loader": "^7.0.0",
    "babel-plugin-istanbul": "^3.0.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-es2016": "^6.16.0",
    "babel-preset-es2017": "^6.16.0",
    "babel-preset-react": "^6.24.1",
    "babelify": "^7.3.0",
    "devtool": "^2.3.0",
    "eslint": "^3.11.1",
    "eslint-plugin-react": "^7.0.1",
    "grunt": "^1.0.1",
    "grunt-babel": "^6.0.0",
    "grunt-browserify": "^5.0.0",
    "grunt-contrib-clean": "^1.0.0",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-contrib-sass": "^1.0.0",
    "grunt-contrib-watch": "^1.0.0",
    "grunt-eslint": "^19.0.0",
    "grunt-mocha": "^1.0.2",
    "grunt-webpack": "^3.0.0",
    "istanbul": "^1.1.0-alpha.1",
    "jsdoc": "^3.4.3",
    "jsdoc-oblivion": "0.0.4",
    "minami": "^1.1.1",
    "mocha": "^3.2.0",
    "plato": "^1.7.0",
    "react-hot-loader": "next",
    "source-map-support": "^0.4.6",
    "webpack": "^2.6.0",
    "webpack-dev-server": "^2.4.5"
  }
}
